{
  "name": "box-console",
  "version": "1.0.0",
  "description": "box console for node",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "types/index.d.ts",
  "scripts": {
    "prepare": "node script/compile.js",
    "prepublishOnly": "node script/build.js",
    "test:cjs": "node examples/index.js",
