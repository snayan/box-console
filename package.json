  "bugs": {
    "url": "https://github.com/snayan/box-console/issues"
  },
  "homepage": "https://github.com/snayan/box-console#readme",
  "devDependencies": {
    "rollup": "^0.66.2",
    "typescript": "^3.1.1"
  },
  "peerDependencies": {
    "chalk": "^2.4.1",
    "string-width": "^2.1.1"
  }
}
